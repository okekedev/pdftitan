{
  "name": "pdftitan",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@adobe/react-spectrum": "^3.42.2",
    "@spectrum-icons/workflow": "^4.2.22",
    "axios": "^1.10.0",
    "googleapis": "^156.0.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "set PORT=3003 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "backend": "cd backend && set PORT=3004 && npm run dev",
    "backend:start": "cd backend && set PORT=3004 && npm start",
    "dev": "concurrently \"npm run backend\" \"set PORT=3003 && react-scripts start\"",
    "dev:backend-only": "cd backend && set PORT=3004 && npm run dev",
    "build:full": "npm run build && cd backend && npm run build",
    "install:all": "npm install && cd backend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3004"
}
