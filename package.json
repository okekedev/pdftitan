{
  "name": "pdftitan",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "pdfjs-dist": "^5.4.449",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "vite --port 3003",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "echo \"No frontend tests\"",
    "backend": "cd backend-py && .venv/bin/uvicorn main:app --reload --port 3004",
    "backend:start": "cd backend-py && .venv/bin/uvicorn main:app --port 3004",
    "dev": "concurrently \"npm run backend\" \"npm start\"",
    "dev:backend-only": "cd backend-py && .venv/bin/uvicorn main:app --reload --port 3004",
    "build:full": "npm run build",
    "install:all": "npm install && cd backend-py && python3 -m venv .venv && .venv/bin/pip install -r requirements.in && .venv/bin/pip freeze > requirements.txt",
    "test:auth": "cd backend-py && .venv/bin/pytest tests/test_env.py tests/test_servicetitan.py tests/test_google_drive.py -v",
    "test:backend": "cd backend-py && .venv/bin/pytest -v"
  },
  "devDependencies": {
    "@types/node": "^25.3.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "typescript": "^5.6.0",
    "vite": "^6.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
